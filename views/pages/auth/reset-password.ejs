<section id="MainWrap">
    <div class="container">

      <div class="row">
        <div class="col-md-3">&nbsp;</div>
        <div class="col-md-6">

          <div class="login_register_wrap">
            
            <div class="col-md-12">
              <div class="login_register">
                  

                  <div class="login_register_form pass_rest_form">
                    <img class="login_register_logo" src="assets/images/logo.png" alt="">
